{
  "name": "pull-request-commitlint-action",
  "version": "0.1.2",
  "private": true,
  "description": "Run commitlint on pull request name.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "build:pack": "npm run build && npm run pack",
    "all": "npm run build && npm run test && npm run format && npm run lint && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Julien Karst",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/types": "^12.1.4",
    "@types/node": "12.20.13",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.23.0",
    "eslint-plugin-github": "3.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "js-yaml": "3.14.1",
    "prettier": "2.2.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0",
    "@commitlint/lint": "^12.1.4",
    "@commitlint/load": "^12.1.4"
  }
}
